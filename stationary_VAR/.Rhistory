init <- dta[dim(dta)[1],]
set.seed(123)
for(i in 1:dim(s.c)[1]){
A<-matrix(s.c[i,1:9],nrow=3,byrow = FALSE)
A.u<-matrix(s.uc[i,1:9],nrow=3,byrow = FALSE)
alp <- s.c[i,10:12]
alp.u <- s.uc[i,10:12]
L <- t(chol(solve(pdmphmc::SPDmatrix.asmatrix(s.c[i,13:18]))))
L.u <- t(chol(solve(pdmphmc::SPDmatrix.asmatrix(s.uc[i,13:18]))))
pr <- init
pr.u <- init
for(j in 1:np){
pr <- A%*%pr + alp #+ L%*%rnorm(3)
pr.u <- A.u%*%pr.u + alp.u #+ L.u%*%rnorm(3)
ip.1.c[i,j] <- pr[1]
ip.1.uc[i,j] <- pr.u[1]
ip.2.c[i,j] <- pr[2]
ip.2.uc[i,j] <- pr.u[2]
ip.3.c[i,j] <- pr[3]
ip.3.uc[i,j] <- pr.u[3]
}
}
qs1.c<-apply(ip.1.c,2,quantile,probs=c(0.05,0.5,0.95))
qs1.uc<-apply(ip.1.uc,2,quantile,probs=c(0.05,0.5,0.95))
qs2.c<-apply(ip.2.c,2,quantile,probs=c(0.05,0.5,0.95))
qs2.uc<-apply(ip.2.uc,2,quantile,probs=c(0.05,0.5,0.95))
qs3.c<-apply(ip.3.c,2,quantile,probs=c(0.05,0.5,0.95))
qs3.uc<-apply(ip.3.uc,2,quantile,probs=c(0.05,0.5,0.95))
